#include ".cache/wal/colors.Xresources"
!! URxvt Appearance
!URxvt.font: xft:Fira Mono:style=Regular:size=11
!NotoEmoji Nerd Font Mono
! üéâ Emoji on üêß Linux
URxvt.font: xft:SauceCodePro Nerd Font Mono:style=regular:size=11 
!URxvt.font: xft:NotoEmoji Nerd Font Mono:style=Book:size=12
!URxvt.font: xft:Source Code Pro for Powerline:style=Regular:size=11, xft:Fira Mono:style=Regular:size=11
URxvt.boldFont: xft:SauceCodePro Nerd Font Mono:style=Bold:size=11
!URxvt.boldFont: xft:Source Code Pro for Powerline:style=Bold:size=11, xft:Fira Mono:style=Bold:size=11
URxvt.italicFont: xft:SauceCodePro Nerd Font Mono:style=Italic:size=11
!URxvt.italicFont: xft:Source Code Pro for Powerline:style=Italic:size=11, xft:Fira Mono:style=Italic:size=11
URxvt.boldItalicfont: xft:SauceCodePro Nerd Font Mono:style=Bold Italic:size=11
!URxvt.boldItalicfont: xft:Source Code Pro for Powerline:style=Bold Italic:size=11, xft:Fira Mono:style=Bold Italic:size=11
URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt.geometry: 92x24
URxvt.internalBorder: 0
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 2048
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.urgentOnBell: true
URxvt.depth: 32
URxvt.transparent: true
URxvt.shading: 15
! set copy/paste binding to ctrl+shift+C/V
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard

! disable old copy/paste binding
URxvt.keysym.Control-Meta-c: builtin-string:
URxvt.keysym.Control-Meta-v: builtin-string:

!URxvt.keysym.F1:    \033[0;68
!URxvt.keysym.F2:    \033[12~ 
!URxvt.keysym.F3:    \[[13~ 
!URxvt.keysym.F4:    \[[14~ 

URxvt.keysym.Control-Up:     \033[1;5A
URxvt.keysym.Control-Down:   \033[1;5B
URxvt.keysym.Control-Left:   \033[1;5D
URxvt.keysym.Control-Right:  \033[1;5C

URxvt.iso14755: false
URxvt.iso14755_52: false

URxvt.perl-lib:~/.local/urxvt/perl/
URxvt.perl-ext-common: default,keyboard-select,resize-font
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search
URxvt.keyboard-select.clipboard:true


